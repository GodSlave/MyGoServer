        "ProcessID": "development",
        "Settings": {
          "TCPAddr": "tcp://0.0.0.0:1883"
        }
      }
    ],
    "User": [
      {
        "Id": "User001",
        "ByteId": 2,
        "ProcessID": "development",
        "Redis": {
          "Uri": "redis://192.168.1.74:6379/1",
          "Queue": "gate-User001"
        }
      }
    ],
    "HttpGate": [
      {
        "Id": "HttpGate001",
        "ByteId": 4,
        "ProcessID": "development",
        "Settings": {
          "listenUrl": "0.0.0.0:8888"
        }
      }
    ],
    "SMS": [
      {
        "Id": "SMS001",
        "ByteId": 5,
